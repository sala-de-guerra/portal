{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "hisorange/browser-detect": "^4.0",
        "jeroennoten/laravel-adminlte": "^3.0.0",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^1.0",
        "phpmailer/phpmailer": "^6.1"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            
            "App\\Classes\\": "app/Classes/",
            "App\\Classes\\GestaoImoveisCaixa\\": "app/Classes/GestaoImoveisCaixa/",
            "App\\Classes\\GestaoImoveisCaixa\\MensagensAutorizacaoContratacao\\": "app/Classes/GestaoImoveisCaixa/MensagensAutorizacaoContratacao/",
            "App\\Classes\\GestaoImoveisCaixa\\MensagensDistrato\\": "app/Classes/GestaoImoveisCaixa/MensagensDistrato/",

            "App\\Http\\Controllers\\": "app/Http/Controllers/",
            "App\\Http\\Controllers\\GestaoImoveisCaixa\\": "app/Http/Controllers/GestaoImoveisCaixa/",

            "App\\Models\\": "app/Models/",
            "App\\Models\\GestaoImoveisCaixa\\": "app/Models/GestaoImoveisCaixa/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
